spring.application.name=E-shopping

# DataBase
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.datasource.url=jdbc:mysql://localhost:3306/es_shopping_platform?useUnicode=true&characterEncoding=utf-8&useSSL=false&serverTimezone=UTC&allowPublicKeyRetrieval=true
spring.datasource.username=your_username
spring.datasource.password=your_password

# MyBatis
mybatis.mapper-locations=classpath:mapper/*.xml
mybatis.type-aliases-package=com.eaxon.eshopping.model.entity
mybatis.configuration.map-underscore-to-camel-case=true

# Server
server.port=8080

# JWT Configuration
jwt.secret=your_jwt_secret_key
jwt.expiration=86400000

# log
logging.level.com.eaxon.eshopping=debug

# DeepSeek AI configuration
spring.ai.openai.base-url=https://api.deepseek.com/v1
spring.ai.openai.api-key=your_api_key
spring.ai.openai.chat.options.model=deepseek-chat 